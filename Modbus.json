    {
        "id": "e692d8aa1fb4d981",
        "type": "function",
        "z": "17ffabd70c78ca8f",
        "name": "Scatter Analog Input",
        "func": "var msgArray = [];\n\nfor (var i=0;i< msg.payload.length;i++) {\n       \n        msgArray[i] = {\n            payload: ((msg.payload[i]*150/27648)-50).toFixed(2)\n             \n        };\n        \n}\n \n\nreturn msgArray;",
